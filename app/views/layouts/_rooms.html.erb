<div class="row">
  <div class="col s12">
    <h3 class="center-align">Room Directory</h3>
  </div>
</div>

<div class="row">
  <%= @rooms.each do |room| %>
    <% @book = Booking.where(:start_time => room.start_at,:end_time => room.end_at).find(:room_id) %>
    <% unless Room.where(:id => @book).exists?%>
      <div class="col s6 m6">
        <div class="card blue-grey darken-1">
          <div class="card-content white-text">
            <span class="card-title">Name: <%= room.name %></span>
            <p>Description: <%= room.description%></p>
          </div>
          <div class="card-action">
            <%= link_to "Show", room_path(room) %>
          </div>
        </div>
      </div>
    <% end %> 
  <% end %>
</div>
       