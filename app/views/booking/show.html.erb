<%= link_to '<i class="material-icon right"></i>Logout'.html_safe, logout_path(@user), class: "waves-effect waves-light btn" %>

<div class="row">
	<div class="col s6 m6">
	  <div class="card blue-grey darken-1">
	    <div class="card-content white-text">
	      <span class="card-title">User: <%= @booking.name %></span>
	      <span class="card-title">Room: <%= @room.name %></span>
	      <p>start_time: <%= @booking.start_time %></p>
	      <p>end_time: <%= @booking.end_time %></p>
	      <p>Status: <%= @booking.status %></p>
	    </div>
	  </div>
	  <%= link_to '<i class="material-icon right"></i>Edit your booking'.html_safe, edit_booking_path(@booking), class: "waves-effect waves-light btn" %>
	  <%= link_to '<i class="material-icon right"></i>Rooms Directory'.html_safe, rooms_path, class: "waves-effect waves-light btn" %>
	</div>
</div>




