<%= form_for(Booking.new, :url => {:controller => :booking, action: :edit}) do |f| %>  
	<%= f.datetime_field :start_time, class: 'form-control' %>
    <%= f.label :start_time %>
    <%= f.datetime_field :end_time, prompt: true,class: 'form-control' %>
    <%= f.label :end_time %>
    
    <%= f.submit "Add", class: "btn btn-primary" %>
<% end %>