<div class="row">
	<div class="col s12">
		<h3 class="center-align">Booking</h3>
	</div>
</div>

<div class="row">
	<%= @bookings.each do |b|%>
	    <div class="col s6 m6">
	      <div class="card blue-grey darken-1">
	        <div class="card-content white-text">
	          <span class="card-title">start_time: <%= b.start_time %></span>
	          <p>end_time: <%= b.end_time%></p>
	          <p>length: <%= b.length %></p>
	          <p>Booking :none</p>
	        </div>
	        <div class="card-action">
	          <a href="booking_index_path">View Profile</a>
	       	</div>
	      </div>
	    </div>
    <% end %>
</div>
