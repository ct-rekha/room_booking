<%= form_for(@booking, html: {class: "col s12"}) do |f| %>
    <%= f.date_field :start_time, class: 'form-control' %>
    <%= f.label :start_time %>
    <%= f.date_field :end_time, class: 'form-control' %>
    <%= f.label :end_time %>
    
    <%= f.submit "Add", class: "btn btn-primary" %>
<% end %>