<%= link_to '<i class="material-icon right"></i>Logout'.html_safe, logout_path(@user), class: "waves-effect waves-light btn" %>

<%= link_to '<i class="material-icon right"></i>BookedRooms'.html_safe,  bookings_path, class: "waves-effect waves-light btn" %>


<%= form_for(@room) do |f| %>
  <%= f.label :start_at, "Start" %>
  <%= f.datetime_field :start_at%>

  <%= f.label :end_at, "End" %>
  <%= f.datetime_field :end_at%>

  <%= f.submit "Add"%>
<% end %>
<%= render 'layouts/rooms' %>


     
    