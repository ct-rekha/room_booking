<%= link_to '<i class="material-icon right"></i>Logout'.html_safe, logout_path(@user), class: "waves-effect waves-light btn" %>

<div class="row">
	<div class="col s12">
		<h3 class="center-align">Available Rooms </h3>
	</div>
</div>

<div class="row">
	<%= @rooms.each do |room| %>
		<% unless Booking.where(:room_id => room.id).exists? %>
			<div class="col s6 m6">
	      <div class="card blue-grey darken-1">
	        <div class="card-content white-text">
	          <span class="card-title">Name: <%= room.name %></span>
	          <p>Description: <%= room.description%></p>
	        </div>
	        <div class="card-action">
	          <%= link_to "Show", room_path(room) %>
	       	</div>
	      </div>
	    </div>
		<% end %> 
  <% end %>
</div>
            
    