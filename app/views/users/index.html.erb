<div class="row">
	<div class="col s12">
		<h3 class="center-align">Users Directory </h3>
	</div>
</div>

<div class="row">
	<%= @users.each do |user|%>
	    <div class="col s6 m6">
	      <div class="card blue-grey darken-1">
	        <div class="card-content white-text">
	          <span class="card-title">Name: <%= user.name %></span>
	          <p>Email: <%= user.email%></p>
	          <p>Course Enrollments :None</p>
	        </div>
	        <div class="card-action">
	          <a href="#">View Profile</a>
	       	</div>
	      </div>
	    </div>
    <% end %>
</div>
            
    